package utility;

import java.util.HashMap;
import java.util.Map;

import dxl_communication.DynamixelControl;

public class FunctionCaller {

	
	/**
	 * Set-functions
	 * @param functionName
	 * @param ID
	 * @param value
	 */
	public void callFunction(String functionName, int id, int value){
		
		
		
	}
	
	/**
	 * Overload: Get-functions
	 * @param functionName 
	 * @param ID
	 */
	public static int callFunction(String functionName, int id){
		
		if (functionName == "getModelNumber") return DynamixelControl.getModelNumber(id);
		
		return 0;
			
	}
	
	
	
